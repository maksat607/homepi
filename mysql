 CREATE TABLE health (
 id INT AUTO_INCREMENT, 
 temperature float(3,1) DEFAULT 37,  
 name VARCHAR(100) DEFAULT 'Maksat Saparbekov',
 pressure VARCHAR(50),
 pulse int,
 date DATETIME DEFAULT (select now()),
 primary key (id) 
 );
